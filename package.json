{
  "name": "nocturnal-scribbles",
  "module": "src/build.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run src/build.ts",
    "dev": "bun run --hot src/dev.ts",
    "test": "bun test",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint && bun test"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "feed": "5.1.0",
    "gray-matter": "4.0.3",
    "marked": "17.0.1",
    "marked-footnote": "1.4.0",
    "marked-gfm-heading-id": "4.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "1.3.3",
    "typescript": "5.9.3"
  }
}
